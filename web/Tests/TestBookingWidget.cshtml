@{
    LcHelpers.TestArea();
    Layout = "_TestLayout.cshtml";
    LcAssets.AddStyle(LcUrl.AppPath + "Styles/dashboard.css");
    LcAssets.AddScript(LcUrl.AppPath + "Scripts/dashboard.js");
}
<h1>Test Booking Widget</h1>
<form method="get">
    <label>BookingRequestID <input type="text" name="BookingRequestID" value="@Request["BookingRequestID"]" /></label>
    <label>BookingID <input type="text" name="BookingID" value="@Request["BookingID"]" /></label>
    <div class="actions">
        <input type="submit" class="main-action" value="Load" />
    </div>
</form>
<div class="DashboardSection">
<div id="main" class="DashboardSection-page">
<div class="test-booking-widget DashboardMessages" style="margin: 4em 0;background:white;padding:2em">
    @RenderPage(LcUrl.RenderLangPath + "Booking/$BookingDetailsWidget.cshtml", new
    {
        ExtraTitleClass = "DashboardSection-page-header",
        UseLinkActions = true
    })
</div>
</div>
</div>