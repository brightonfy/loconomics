@{
    var name = PageData[0];
    
    var checked1 = PageData[1] != "1" ? "" : "checked='checked'";
    var checked2 = PageData[1] != "2" ? "" : "checked='checked'";
    var checked3 = PageData[1] != "3" ? "" : "checked='checked'";
    var checked4 = PageData[1] != "4" ? "" : "checked='checked'";
    var checked5 = PageData[1] != "5" ? "" : "checked='checked'";

    var disabled = Convert.ToBoolean(PageData[2]) ? "disabled='disabled'" : "";
 }
<br />
<input name="@name" type="radio" class="star" value="1" @checked1 @disabled/>
<input name="@name" type="radio" class="star" value="2" @checked2/>
<input name="@name" type="radio" class="star" value="3" @checked3/>
<input name="@name" type="radio" class="star" value="4" @checked4/>
<input name="@name" type="radio" class="star" value="5" @checked5/>

