@{

}

<div id="container" class="search clearfix">
 <div id="main" class="">

     <div id="jobTitles" class="current tab-body">
			<ul class="search-results">
            @foreach(var c in posrow){
				<li class="clearfix">
					<div class="id col">
	               <img src="../@c.Photo" alt="" />
					</div>
					<div class="name col">
						<h3><a href="../en_US/ProviderProfile.cshtml?ProviderID=@c.ProfileID&PositionId=@c.PositionID&Position=&ServiceType=@serviceType&Service=@LC_Cat">@c.FirstName @c.LastName</a></h3>
						<blockquote>"Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua..."  <a href="#">more</a></blockquote>
					</div>
				   <div class="lang col">
                        @Html.Label("rating1 ")
                        @RenderPage("~/en_US/StarRating.cshtml", rating1data)
                        @Html.Label("rating2 ")
                        @RenderPage("~/en_US/StarRating.cshtml", rating2data)
                        @Html.Label("rating3 ")
                        @RenderPage("~/en_US/StarRating.cshtml", rating3data)
                        @Html.Label("rating4 ")
                        @RenderPage("~/en_US/StarRating.cshtml", rating4data)
					</div>
					<div class="buttons">
						<a class="button fanciful" href="../en_US/ProviderProfile.cshtml?ProviderID=@c.ProfileID&PositionId=@c.PositionID&Position=&ServiceType=@serviceType&Service=@LC_Cat">View full profile</a>  
                        <a class="button fanciful" href="#">Book for $35/hour</a>
					</div>
					<!-- <h3 class="job-title"><a class="job-status inactive" href="ProviderProfile.cshtml?ProviderID=@c.ProfileID&PositionId=@c.PositionID&Position="> </a></h3>
					<table class="profile-stats">
						<tr><td>Profile Views:</td><td>0</td></tr>
						<tr><td>Last Updated:</td><td>@c.UpdatedDate.ToString("dd MMM yyyy")</td></tr>
					</table>-->
				</li>
			}
			</ul>	
    </div><!-- #jobTitles -->
  </div><!-- #main -->
	<div id="secondary">
		<form id="searchAgain" name="searchAgain" class="fancy">
			<legend>Refine results</legend>
			<fieldset>
				<label for="serviceType">Keywords </label>
            <input type="text" name="serviceType" id="serviceType" value="[current search terms]" />
			</fieldset>
			<fieldset class="search-params">
				<fieldset>
					<label for="location">You're searching for services near:</label>
					<select id="location">
						<option>Home</option>
						<option>Office</option>
					</select>
				</fieldset>
				<fieldset>
					<label for="date">Preferred Date</label>
					<input type="text" id="date" class="date-pick" value="06/07/2011" />
				</fieldset>
			</fieldset>
			<fieldset>
				<label for="serviceDetails">Service Details:</label>
				<fieldset class="checklist" id="serviceDetails">
					<fieldset><input type="checkbox" value="lorem" id="hk1" /> <label for="hk1">Lorem ipsum dolor</label></fieldset>
					<fieldset><input type="checkbox" value="ipsum" id="hk2" /> <label for="hk2">Consectitur</label></fieldset>
					<fieldset><input type="checkbox" value="dolor" id="hk3" /> <label for="hk3">Nullam sed ante leo</label></fieldset>
					<fieldset><input type="checkbox" value="sit" id="hk4" /> <label for="hk4">Etiam sodales </label></fieldset>
					<fieldset><input type="checkbox" value="amet" id="hk5" /> <label for="hk5">Sit amet</label></fieldset>
				</fieldset>
			</fieldset>
			<fieldset>
				<label for="education">Education:</label>
				<fieldset class="checklist" id="serviceDetails">
					<fieldset><input type="checkbox" value="lorem" id="hk1" /> <label for="hk1">Lorem ipsum dolor</label></fieldset>
					<fieldset><input type="checkbox" value="ipsum" id="hk2" /> <label for="hk2">Consectitur</label></fieldset>
					<fieldset><input type="checkbox" value="dolor" id="hk3" /> <label for="hk3">Nullam sed ante leo</label></fieldset>
					<fieldset><input type="checkbox" value="sit" id="hk4" /> <label for="hk4">Etiam sodales </label></fieldset>
					<fieldset><input type="checkbox" value="amet" id="hk5" /> <label for="hk5">Sit amet</label></fieldset>
				</fieldset>
			</fieldset>
			<button class="search-again fanciful" type="submit">Search</button>
		</form>
	</div><!-- #secondary -->

</div><!-- #container -->
