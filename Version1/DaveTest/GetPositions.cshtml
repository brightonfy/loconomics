
    @{
    var db = Database.Open("Loconomics");
    var sql = "SELECT distinct PositionID,PositionSingular ";
        sql +="FROM positions ";
        sql +="WHERE PositionPlural LIKE CONCAT('%', @0,'%')  OR ";
        sql +="PositionSingular LIKE CONCAT('%', @0,'%')  ;";
    var term = Request["titleSearch"] + "%";
    var result = db.Query(sql, term);
    var data = result.Select(p => new{ label = p.PositionSingular,id = p.PositionID});
    Json.Write(data, Response.Output);
}
    