@{
    


    Layout = "~/en_ES/_SiteLayout.cshtml";
    Page.Title = "Search Results...";
    
     var db = Database.Open("sqlloco");
     var LC_Term  = Request["serviceType"] + "%";
     var LC_Cat = Request["Service"];
     var searchsql = "EXEC dbo.GetSearchResults @0,@1,@2,@3";
     var posrow = db.Query(searchsql,1,2,LC_Term,LC_Cat);

}


<div id="container" class="search clearfix">
 <div id="main" class="">
		<h1>@Page.Title</h1>
     <div id="jobTitles" class="current tab-body">
			<ul class="search-results">
            @foreach(var c in posrow){    
				<li class="clearfix">
					<div class="id col">
	               <img src="../@c.Photo" alt="" />
						<h3><a href="../en_ES/ProviderProfile.cshtml?ProviderID=@c.ProfileID&PositionId=@c.PositionID&Position=@c.PositionSingular">@c.FirstName @c.LastName</a></h3>
					</div>
				   <div class="lang col">
						<table>
							<tr>
								<td>
								    <input name=@c.ProfileID+@c.PositionID+1 type="radio" class="star" disabled="disabled"/>
                                    <input name=@c.ProfileID+@c.PositionID+1 type="radio" class="star"/>
                                    <input name=@c.ProfileID+@c.PositionID+1 type="radio" class="star" checked="checked"/>
                                    <input name=@c.ProfileID+@c.PositionID+1 type="radio" class="star"/>
                                    <input name=@c.ProfileID+@c.PositionID+1 type="radio" class="star"/>
								</td>
								<th>Professionalism</th>
							</tr>
							<tr>
								<td>
									<input name=@c.ProfileID+@c.PositionID+2 type="radio" class="star" disabled="disabled"/>
                                    <input name=@c.ProfileID+@c.PositionID+2 type="radio" class="star" checked="checked"/>
                                    <input name=@c.ProfileID+@c.PositionID+2 type="radio" class="star"/>
                                    <input name=@c.ProfileID+@c.PositionID+2 type="radio" class="star"/>
                                    <input name=@c.ProfileID+@c.PositionID+2 type="radio" class="star"/>
								</td>
								<th>Attention to detail</th>
							</tr>
							<tr>
								<td>
								    <input name=@c.ProfileID+@c.PositionID+3 type="radio" class="star" disabled="disabled"/>
                                    <input name=@c.ProfileID+@c.PositionID+3 type="radio" class="star"/>
                                    <input name=@c.ProfileID+@c.PositionID+3 type="radio" class="star"/>
                                    <input name=@c.ProfileID+@c.PositionID+3 type="radio" class="star"/>
                                    <input name=@c.ProfileID+@c.PositionID+3 type="radio" class="star" checked="checked"/>
								</td>
								<th>Trustworthiness</th>
							</tr>
						</table>
						<p>Speaks French, Spanish, English</p>
					</div>
					<div class="book col">
						<p class="rate fancy">25 €/hr</p>
						<a class="button fanciful" href="../en_ES/ProviderProfile.cshtml?ProviderID=@c.ProfileID&PositionId=@c.PositionID&Position=@c.PositionSingular">View full profile and book</a>
					</div>
					<div class="review col">
						<img src="../img/eg-profile-pic.jpg" alt="" />
						<blockquote>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua...<a href="#">more</a></blockquote>
					</div>
					<!-- <h3 class="job-title"><a class="job-status inactive" href="ProviderProfile.cshtml?ProviderID=@c.ProfileID&PositionId=@c.PositionID&Position=@c.PositionSingular">@c.PositionSingular </a></h3>
					<table class="profile-stats">
						<tr><td>Profile Views:</td><td>0</td></tr>
						<tr><td>Last Updated:</td><td>@c.UpdatedDate.ToString("dd MMM yyyy")</td></tr>
					</table>-->
				</li>
				}
				
			</ul>	
    </div><!-- #jobTitles -->
  </div><!-- #main -->
	<div id="secondary">
		<form id="searchAgain" name="searchAgain" class="fancy">
			<legend>Search Again</legend>
			<fieldset class="search-params">
				<fieldset>
					<label for="location">You're searching for services near:</label>
					<select id="location">
						<option>Home</option>
						<option>Office</option>
					</select>
				</fieldset>
				<fieldset>
					<label for="date">Preferred Date</label>
					<input type="text" id="date" class="date-pick" value="06/07/2011" />
				</fieldset>
			</fieldset>
			<fieldset>
				<label for="serviceDetails">Service Details:</label>
				<fieldset class="checklist" id="serviceDetails">
					<fieldset><input type="checkbox" value="lorem" id="hk1" /> <label for="hk1">Lorem ipsum dolor</label></fieldset>
					<fieldset><input type="checkbox" value="ipsum" id="hk2" /> <label for="hk2">Consectitur</label></fieldset>
					<fieldset><input type="checkbox" value="dolor" id="hk3" /> <label for="hk3">Nullam sed ante leo</label></fieldset>
					<fieldset><input type="checkbox" value="sit" id="hk4" /> <label for="hk4">Etiam sodales </label></fieldset>
					<fieldset><input type="checkbox" value="amet" id="hk5" /> <label for="hk5">Sit amet</label></fieldset>
				</fieldset>
			</fieldset>
			<fieldset>
				<label for="education">Education:</label>
